<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ucf.assignments.Controller">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <MenuBar prefHeight="18.0" prefWidth="105.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Import List" />
                  <MenuItem mnemonicParsing="false" text="Export List" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="View">
            <items>
              <MenuItem mnemonicParsing="false" text="View List" />
            </items>
          </Menu>
        </menus>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </MenuBar>
      <Text layoutX="94.0" layoutY="43.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Add New Todo">
         <font>
            <Font size="30.0" />
         </font>
      </Text>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Title" />
      <TextField fx:id="inputTitle" prefWidth="149.0" />
      <Text layoutX="93.0" layoutY="43.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Description" />
      <TextField fx:id="inputDescription" layoutX="30.0" layoutY="66.0" prefWidth="149.0" />
      <Text layoutX="94.0" layoutY="124.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Due Date" />
      <DatePicker fx:id="inputDuedate" />
      <Text layoutX="114.0" layoutY="311.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Status" />
      <ChoiceBox fx:id="inputStatus" prefWidth="150.0" />
      <Button fx:id="newSave" mnemonicParsing="false" onAction="#newSavetodo" text="Save" />
   </children>
</VBox>
